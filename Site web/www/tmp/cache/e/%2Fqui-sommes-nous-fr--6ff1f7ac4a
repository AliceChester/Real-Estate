function loadCKEditor() {
	var prefix_id=$(this).attr('id');
	var ajaxload=[["textarea.crayon-active","Full"],["textarea.inserer_barre_edition","Full"]];
	if (prefix_id){
		$.each(ajaxload, function(i){
			ajaxload[i][2]=prefix_id;
			ajaxload[i][0]='#'+prefix_id+' '+ajaxload[i][0];
		});
	}
	fullInitCKEDITOR(ajaxload) ;
}
$(function(){
	if(typeof onAjaxLoad == 'function') onAjaxLoad(loadCKEditor);
	fullInitCKEDITOR([["#formulaire_forum textarea[name=texte]","Full"],["textarea.inserer_barre_edition","Full"]]);
}) ;
